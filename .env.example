# OPENAI相关
OPENAI_API_BASE_URL='http://127.0.0.1:4000'
OPENAI_API_KEY='copy from llmlite/llmlite.yaml.general_settings.master_key'

# AUTOMATIC1111_BASE_URL="http://localhost:7860"

